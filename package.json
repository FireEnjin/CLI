{
  "name": "@fireenjin/cli",
  "version": "1.1.11",
  "description": "A set of tools for speeding up production of full-stack Firebase projects.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rimraf dist/",
    "lint": "tslint --project tsconfig.json",
    "plop": "plop",
    "publish": "npm run build",
    "refresh": "rimraf ./node_modules ./package-lock.json && npm install",
    "semantic-release": "semantic-release",
    "serve": "tsc -w",
    "start": "npm run serve",
    "test": "jest --no-cache",
    "test:watch": "jest --watch --no-cache"
  },
  "bin": {
    "fireenjin": "dist/index.js"
  },
  "dependencies": {
    "@fireenjin/graphql": "^1.1.10",
    "apollo-server": "^3.12.1",
    "firebase-admin": "^11.11.0",
    "handlebars": "^4.7.8",
    "inquirer": "^9.2.11",
    "json-logic-js": "^2.0.2",
    "plop": "^4.0.0",
    "pluralize": "^8.0.0",
    "prettier": "^3.0.3",
    "recursive-copy": "^2.0.14",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.5",
    "tiny-glob": "^0.2.9",
    "type-graphql": "^1.1.1",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@semantic-release/commit-analyzer": "^11.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^11.0.0",
    "@semantic-release/release-notes-generator": "^12.0.0",
    "@types/jest": "^29.5.6",
    "jest": "^29.7.0",
    "semantic-release": "^22.0.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.2",
    "tslint": "^6.1.3",
    "typescript": "^5.2.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FireEnjin/CLI.git"
  },
  "keywords": [
    "firebase",
    "github",
    "madness",
    "labs",
    "popcorn245",
    "youtube",
    "javascript",
    "typescript"
  ],
  "author": "Madness Labs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FireEnjin/CLI/issues"
  },
  "homepage": "https://github.com/FireEnjin/CLI#readme",
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(ts?)$",
    "testURL": "http://localhost",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "private": false
}