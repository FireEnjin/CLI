{
  "name": "@fireenjin/cli",
  "version": "1.1.2",
  "description": "A set of tools for speeding up production of full-stack Firebase projects.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rimraf dist/",
    "lint": "tslint --project tsconfig.json",
    "plop": "plop",
    "publish": "npm run build",
    "refresh": "rimraf ./node_modules ./package-lock.json && npm install",
    "semantic-release": "semantic-release",
    "serve": "tsc -w",
    "start": "npm run serve",
    "test": "jest --no-cache",
    "test:watch": "jest --watch --no-cache"
  },
  "bin": {
    "fireenjin": "dist/index.js"
  },
  "dependencies": {
    "@fireenjin/graphql": "^1.1.6",
    "apollo-server": "^3.6.7",
    "firebase-admin": "^11.0.1",
    "handlebars": "^4.7.7",
    "inquirer": "^9.1.1",
    "json-logic-js": "^2.0.2",
    "plop": "^3.1.1",
    "pluralize": "^8.0.0",
    "prettier": "^2.7.1",
    "recursive-copy": "^2.0.14",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "tiny-glob": "^0.2.9",
    "type-graphql": "^1.1.1",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^9.0.1",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@types/jest": "^28.1.1",
    "jest": "^28.1.1",
    "semantic-release": "^19.0.5",
    "ts-jest": "^28.0.4",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.0",
    "tslint": "^6.1.3",
    "typescript": "^4.8.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FireEnjin/CLI.git"
  },
  "keywords": [
    "firebase",
    "github",
    "madness",
    "labs",
    "popcorn245",
    "youtube",
    "javascript",
    "typescript"
  ],
  "author": "Madness Labs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FireEnjin/CLI/issues"
  },
  "homepage": "https://github.com/FireEnjin/CLI#readme",
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(ts?)$",
    "testURL": "http://localhost",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "private": false
}
